{
  "entry": [
    "src/main.ts",
    "src/modules/**/*.module.ts"
  ],
  "project": [
    "src/**/*.ts"
  ],
  "ignore": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "files": "error",
    "dependencies": "error",
    "unlisted": "error",
    "exports": "error",
    "types": "error",
    "enumMembers": "error",
    "classMembers": "error",
    "duplicates": "error"
  }
}
